{
  "name": "next-directus",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "concurrently \"yarn start-directus\" \"yarn next-dev\"",
    "start": "yarn start-directus && yarn next-start",
    "build": "yarn start-directus && next build",
    "start-directus": "cd directus && npx directus start",
    "next-dev": "next dev --experimental-https",
    "next-start": "next start",
    "lint": "next lint"
  },
  "dependencies": {
    "@directus/sdk": "^12.0.1",
    "@radix-ui/react-label": "^2.0.2",
    "@radix-ui/react-slot": "^1.0.2",
    "@types/js-cookie": "^3.0.4",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.0.0",
    "js-cookie": "^3.0.5",
    "lucide-react": "^0.284.0",
    "next": "^13.5.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tailwind-merge": "^1.14.0",
    "tailwindcss-animate": "^1.0.7"
  },
  "devDependencies": {
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "autoprefixer": "^10",
    "concurrently": "^8.2.1",
    "eslint": "^8",
    "eslint-config-next": "^13.5.4",
    "postcss": "^8",
    "tailwindcss": "^3",
    "typescript": "^5"
  }
}
